    "dotenv": "4.0.0",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.4.0",
    "graphql-cli": "2.12.4",
    "nodemon": "1.14.11",
    "npm-run-all": "4.1.2",
    "prettier": "^1.9.2",
    "prisma-cli": "1.0.0"
